application: go-socialcounters
version: 7
runtime: go
api_version: go1

env_variables:
  ADS: ''
  WHITELIST: ''
  BLACKLIST: ''
  FACEBOOK_APP_ID: ''
  FACEBOOK_APP_SECRET: ''
  TTL_MEMORY: 86400
  TTL_DEFAULT: 300
  TTL_RESTRICTED: 60
  REFRESH_TTL_LEFT_THRESHOLD: 10
  REFRESH_BATCH_SIZE: 20
  VERBOSE: 0

handlers:
- url: /.*
  script: _go_app

nobuild_files: (Godeps|services|utils|web|main.go)
